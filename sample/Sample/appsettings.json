{
  "Serilog": {
    "Using":  ["Serilog.Sinks.Literate"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "LiterateConsole" },
      { "Name": "File", "Args": { "path": "%TEMP%\\Logs\\serilog-configuration-sample.txt" } }
    ],
    "Enrich": {
      "With": [
        "FromLogContext",
        "WithMachineName",
        "WithThreadId"
      ],
      "WithProperties": {
        "Application": "Sample"
      }
    }
  }
}