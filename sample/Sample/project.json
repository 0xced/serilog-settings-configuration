  "description": "Sample Console Application",
  "authors": [ "nblumhardt" ],
  "buildOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
    "Serilog.Settings.Configuration": {"target": "project"},
